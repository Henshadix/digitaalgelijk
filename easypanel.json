{
  "name": "digitaalgelijk-website",
  "type": "docker-compose",
  "compose": {
    "file": "docker-compose.yml"
  },
  "domains": [
    {
      "domain": "digitaalgelijk.nl",
      "port": 3002,
      "https": true,
      "path": "/"
    }
  ],
  "env": [
    {
      "name": "NODE_ENV",
      "value": "production"
    },
    {
      "name": "PORT",
      "value": "3000"
    },
    {
      "name": "DOMAIN",
      "value": "digitaalgelijk.nl"
    }
  ],
  "resources": {
    "memory": 1024,
    "cpu": 1
  }
}
